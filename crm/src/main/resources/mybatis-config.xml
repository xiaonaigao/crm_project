<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--打印日志-->
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>
    <!--类起别名-->
    <typeAliases>
        <package name="com.wzl.crm.workbench.domain"/>
    </typeAliases>
    <mappers>
        <package name="com.wzl.crm.settings.mapper"/>
        <package name="com.wzl.crm.workbench.mapper"/>
    </mappers>
</configuration>
